{
    "manifest_version": 3,
    "name": "Authenic Movie Ratings",
    "version": "1.0",
    "content_scripts": [
        {
          "matches": ["https://www.netflix.com/*"],
          "js": ["./main.js"]
        }
      ],
      "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
      }
}